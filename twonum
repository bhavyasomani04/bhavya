#include<stdio.h>
int input()
{
        int a;
        scanf("%d",&a);
        return a;
}
int input_2()
{
        int b;
        scanf("%d",&b);
        return b;
}
int add(int a,int b)
{
        int c;
        c=a+b;
        return c;
}
void output(int c)

{

        printf("the additon of tow no. is %d",c);
}
int main()
{
        int a=input();
        int b=input_2();
        int c=add(a,b);
        output(c);
        return 0;
}

